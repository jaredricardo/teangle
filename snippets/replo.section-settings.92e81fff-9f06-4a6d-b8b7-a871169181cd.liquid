    {%- capture default_replo_text_719a2ef5-cdbc-4f14-a113-ac88c5496186 -%}<p>How does Teangle Matcha differ from regular green tea?</p>{%- endcapture -%}
    {%- assign replo_text_719a2ef5-cdbc-4f14-a113-ac88c5496186 = replo_text_719a2ef5-cdbc-4f14-a113-ac88c5496186 | default: default_replo_text_719a2ef5-cdbc-4f14-a113-ac88c5496186 -%}
