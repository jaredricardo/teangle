    {%- capture default_replo_text_bb87acab-bb39-4984-aecf-ec36f3ec701e -%}<p>Is Teangle Matcha suitable for individuals with caffeine sensitivity?</p>{%- endcapture -%}
    {%- assign replo_text_bb87acab-bb39-4984-aecf-ec36f3ec701e = replo_text_bb87acab-bb39-4984-aecf-ec36f3ec701e | default: default_replo_text_bb87acab-bb39-4984-aecf-ec36f3ec701e -%}
