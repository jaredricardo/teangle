    "replo_homepage_4_reasons_MmMDTQ": {
      "type": "replo-homepage-4-reasons",
      "name": "Homepage 4 Reasons",
      "settings": {
        "replo_text_af82958b-994b-4270-9ba5-ad92ec429e54": "<p>4 Reasons You’ll Never Go Back to Coffee</p>",
